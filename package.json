{
  "name": "ng2-dragula",
  "version": "1.1.6",
  "description": "Simple drag and drop with dragula",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valor-software/ng2-dragula.git"
  },
  "keywords": [
    "angular2",
    "ng2",
    "dragula",
    "angular2-dragula",
    "ng2-dragula"
  ],
  "author": "NathanWalker <NathanWalker>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/valor-software/ng2-dragula/issues"
  },
  "homepage": "https://github.com/valor-software/ng2-dragula#readme",
  "scripts": {
    "clean": "rimraf node_modules doc dist && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean && npm start",
    "watch": "webpack --watch --progress --profile",
    "build": "rimraf dist && webpack --progress --profile",
    "server": "webpack-dev-server --inline --progress --port 3000",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "lint": "tsconfig-lint",
    "e2e": "protractor",
    "e2e-live": "protractor --elementExplorer",
    "test": "karma start",
    "test-watch": "karma start --no-single-run --auto-watch",
    "ci": "npm run e2e && npm run test",
    "docs": "typedoc --options typedoc.json src/app/app.ts",
    "start": "npm run server",
    "postinstall": "npm run webdriver-update && typings install"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.13",
    "dragula": "^3.6.8",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "0.6.10"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.2",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.4",
    "html-loader": "^0.4.0",
    "html-webpack-plugin": "^2.8.1",
    "istanbul-instrumenter-loader": "^0.1.3",
    "jasmine-core": "^2.3.4",
    "jasmine-spec-reporter": "^2.4.0",
    "json-loader": "^0.5.3",
    "karma": "0.13.19",
    "karma-chrome-launcher": "^0.2.1",
    "karma-coverage": "^0.5.5",
    "karma-jasmine": "^0.3.7",
    "karma-phantomjs2-launcher": "^0.5.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.7.0",
    "node-sass": "^3.4.2",
    "null-loader": "git+https://github.com/webpack/null-loader.git",
    "postcss-loader": "^0.8.0",
    "protractor": "^3.1.1",
    "raw-loader": "0.5.1",
    "rimraf": "^2.5.1",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "systemjs-builder": "^0.15.15",
    "ts-loader": "^0.8.1",
    "tsconfig-lint": "^0.7.0",
    "tslint": "^3.4.0",
    "tslint-loader": "^2.1.0",
    "typedoc": "^0.3.12",
    "typescript": "^1.7.3",
    "typings": "^0.8.1",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.15",
    "webpack-dev-server": "^1.14.1"
  }
}